    "@emotion/styled": "^11.11.0",
    "@opengpts/ui": "workspace:^",
    "@plasmohq/messaging": "0.6.0",
    "@plasmohq/storage": "^1.9.0",
    "@rahsheen/react-wizard": "^2.1.2",
    "@supabase/auth-ui-react": "^0.4.6",
