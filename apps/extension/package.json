    "postcss-import": "^15.1.0",
    "prettier": "3.0.3",
    "tailwindcss": "^3.3.6",
    "typescript": "5.2.2"
  },
  "manifest": {
    "default_locale": "en",
    "permissions": [
      "tabs",
      "sidePanel",
      "scripting",
      "webRequest",
      "declarativeNetRequest",
      "<all_urls>"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "options.html"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ],
    "key": "$CRX_KEY",
    "host_permissions": [
      "*://*/*",
      "<all_urls>"
    ],
    "externally_connectable": {
      "matches": [
        "*://*.localhost.com/*"
      ]
    }
  }
}