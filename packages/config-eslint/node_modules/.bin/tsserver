#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hzy/Code/chatgptThing/madan/app/OpenGPTS/my-turborepo/node_modules/.pnpm/registry.npmmirror.com+typescript@5.3.3/node_modules/typescript/bin/node_modules:/Users/hzy/Code/chatgptThing/madan/app/OpenGPTS/my-turborepo/node_modules/.pnpm/registry.npmmirror.com+typescript@5.3.3/node_modules/typescript/node_modules:/Users/hzy/Code/chatgptThing/madan/app/OpenGPTS/my-turborepo/node_modules/.pnpm/registry.npmmirror.com+typescript@5.3.3/node_modules:/Users/hzy/Code/chatgptThing/madan/app/OpenGPTS/my-turborepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hzy/Code/chatgptThing/madan/app/OpenGPTS/my-turborepo/node_modules/.pnpm/registry.npmmirror.com+typescript@5.3.3/node_modules/typescript/bin/node_modules:/Users/hzy/Code/chatgptThing/madan/app/OpenGPTS/my-turborepo/node_modules/.pnpm/registry.npmmirror.com+typescript@5.3.3/node_modules/typescript/node_modules:/Users/hzy/Code/chatgptThing/madan/app/OpenGPTS/my-turborepo/node_modules/.pnpm/registry.npmmirror.com+typescript@5.3.3/node_modules:/Users/hzy/Code/chatgptThing/madan/app/OpenGPTS/my-turborepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/registry.npmmirror.com+typescript@5.3.3/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/registry.npmmirror.com+typescript@5.3.3/node_modules/typescript/bin/tsserver" "$@"
fi
